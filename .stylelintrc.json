{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-prettier"
  ],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "mixin",
          "define-mixin",
          "media",
          "import",
          "custom-selector",
          "custom-media",
          "font-face",
          "import-sanitize"
        ]
      }
    ],
    "block-no-empty": null,
    "no-duplicate-selectors": null,
    "max-empty-lines": 1
  }
}